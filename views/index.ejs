<div class="author">
    <h1 class="author__heading"> Hi, I am Duc Quyen</h1>
    <p class="author__body">Student Form HUMG</p>
</div>

<img src="/img/hero-image.jpg" alt="person looking out through window" class="hero-image" width="981" height="528">

<section class="articles">
    <h2 class="articles__heading"></h2>
    <ul class="article-ul">
        <% data.forEach(post => { %>
        <li>
            <a href="/post/<%= post._id  %>">
                <span><%=post.title  %></span>
                <span class="article-list__date"><%= post.createAt.toDateString()%></span>
            </a>
        </li>
        <% }) %>
    </ul>

    <% if (nextPage !== null) { %>
        <a href="/?page=<%= nextPage  %>" class="pagination">&lt; View Older Post</a>
    <% } %>





</section>